{
  "name": "@darkflare/new",
  "version": "3.0.4",
  "description": "A simple way to setup a darkflare app.",
  "bin": {
    "@darkflare/new": "./dist/index.js",
    "create-darkflare-app": "./dist/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --legal-comments=none --platform=node --format=cjs --outfile=dist/index.js",
    "build:watch": "esbuild src/index.ts --bundle --minify --watch --legal-comments=none --platform=node --format=cjs --outfile=dist/index.js",
    "test": "",
    "publish:canary": "npm version prerelease --preid=canary && npm publish --tag canary --access public",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/azury"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azurydev/darkflare.git"
  },
  "keywords": [
    "api",
    "workers",
    "cloudflare",
    "framework",
    "darkflare",
    "cli",
    "installer"
  ],
  "author": "Azury <hello@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/azurydev/darkflare/issues"
  },
  "homepage": "https://github.com/azurydev/darkflare#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "chalk": "^5.0.1",
    "esbuild": "^0.14.39",
    "fs-extra": "^10.1.0",
    "typescript": "^4.6.4",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
